################################################################################
# Top-level Dependencies:
################################################################################

all: cv
clean:
	rm -f *dvi *bak *pdf *log *lot *toc *aux *.out *.bbl *.blg

################################################################################
# CV Dependencies:
################################################################################

TEX = pdflatex
BIB = bibtex

cv: $(OBJECTS) 
	$(TEX) cv
	$(BIB) cv
	$(BIB) cf
	$(BIB) ws
	$(BIB) tr
	$(TEX) cv
	$(TEX) cv
